<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="id.gits.app.sqlitemultiple" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme"
        >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <!-- SpesifikasiMobilActivity -->
        <activity
            android:name=".SpesifikasiMobilActivity"
            android:label="@string/add_SpesifikasiMobil"
            android:parentActivityName=".MainActivity"
            >
        </activity>

        <!-- HargaMobilActivity -->
        <activity
            android:name=".HargaMobilActivity"
            android:label="@string/add_HargaMobil"
            android:parentActivityName=".MainActivity"
            >
        </activity>

        <!-- DaftarHargaMobilActivity -->
        <activity
            android:name=".DaftarHargaMobilActivity"
            android:label="@string/add_DaftarHargaMobil"
            android:parentActivityName=".MainActivity"
            >
        </activity>
    </application>

</manifest>
